CREATE PROCEDURE ProcWithSelfAssignments
AS 
BEGIN
declare @a int = 0;
update TableA
set a = a+1             -- picked up
, @a = @a +1            -- picked up

set @a=@a+1;            -- picked up
set @a+=1;              -- not picked up
set @a=1+@a;            -- picked up
set @a=1-@a;            -- not picked up

set @a=@a % 1;          -- picked up

update TableA
set a = a-1             -- picked up
, @a = @a-1             -- picked up
, a = 1-a               -- not picked up
, @a = 1-@a             -- not picked up

update TableA
set a = a/1             -- picked up
, @a = @a/1             -- picked up
, a = 1/a               -- not picked up
, @a = 1/@a             -- not picked up

update TableA
set a = a % 1             -- picked up
, @a = @a % 1             -- picked up
, a = 1 % a               -- not picked up
, @a = 1 % @a             -- not picked up

update TableA
set a = a & 1             -- picked up
, @a = @a & 1             -- picked up
, a = 1 & a               -- picked up
, @a = 1 & @a             -- picked up

update TableA
set a = a | 1             -- picked up
, @a = @a | 1             -- picked up
, a = 1 | a               -- picked up
, @a = 1 | @a             -- picked up

update TableA
set a = a ^ 1             -- picked up
, @a = @a ^ 1             -- picked up
, a = 1 ^ a               -- picked up
, @a = 1 ^ @a             -- picked up
, @a ^= 1                 -- not picked up

/* not a brilliant example - it won't compile in practice */
update t1 
set a = a+1                 -- picked up
,   b = t2.b+1              -- picked up
,   t1.c = t2.c+1           -- not picked up
,   t1.e = e+1              -- picked up
from TableA t1
join TableA t2
on  t1.a=t2.a

END
