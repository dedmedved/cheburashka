create proc PreferDropIfExistsRule_ProcWithDropIfExists
as

drop TABLE if exists dbo.table1;
drop view if exists view1;
drop synonym if exists syn1;
drop function if exists fn1;
drop proc if exists proc1;
drop trigger if exists trg1;
drop sequence if exists seq1;
--drop EXTERNAL TABLE if exists extTable1 ; 
drop SECURITY POLICY if exists secPol1;
drop RULE if exists rul1;
drop DEFAULT if exists dft1;  
drop AGGREGATE if exists agg1;  

GO  